{
	"name": "markdown-toc",
	"displayName": "markdown-toc",
	"description": "Generate Table of Contents for Markdown documents",
	"version": "1.0.0",
	"publisher": "joffreykern",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
  "license": "SEE LICENSE.txt",
	"activationEvents": [
		"onCommand:extension.markdownToc",
    "onLanguage:markdown"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [{
			"command": "extension.markdownToc",
			"title": "Generate TOC for Markdown"
		}]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	},
  "repository": {
		"type": "git",
		"url": "https://github.com/curioustechizen/vscode-markdown-toc.git"
	},
  "bugs": {
		"url": "https://github.com/curioustechizen/vscode-markdown-toc/issues"
	}
}